{
  "kind": "collectionType",
  "collectionName": "mascot_dialogues",
  "info": {
    "singularName": "mascot-dialogue",
    "pluralName": "mascot-dialogues",
    "displayName": "Mascot Dialogue"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "mascotState": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::mascot.mascot",
      "inversedBy": "mascot_dialogues"
    },
    "context": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "default": "default",
      "enum": [
        "default",
        "achievement",
        "streak",
        "reminder",
        "error",
        "onboarding"
      ]
    },
    "message": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "maxLength": 500,
      "required": true
    },
    "language": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "default": "en",
      "enum": [
        "en",
        "vi",
        "fr"
      ]
    },
    "priority": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": 0,
      "min": 0,
      "max": 100
    },
    "active": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "default": true
    },
    "startDate": {
      "type": "datetime",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "endDate": {
      "type": "datetime",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
